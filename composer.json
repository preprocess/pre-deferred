{
    "type": "pre-macro",
    "name": "pre/deferred",
    "license": "MIT",
    "require": {
        "pre/plugin": "*"
    },
    "autoload": {
        "psr-4": {
            "Pre\\Deferred\\": "source"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload-dev": {
        "files": [
            "tests/stubs.php"
        ]
    },
    "extra": {
        "macros": [
            "source/macros.yay"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pre-plugin",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
